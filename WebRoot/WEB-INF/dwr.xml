<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN" 
"http://getahead.org/dwr/dwr30.dtd">

<dwr>
	<allow>
		<create creator="new" javascript="testdwr">
			<param name="class" value="com.test.dwr.TestDwr" />
		</create>
		<create creator="spring" javascript="loginaction">
			<param name="beanName" value="loginAction" />
		</create>
		<create creator="spring" javascript="registeraction">
			<param name="beanName" value="registerAction" />
		</create>
		<create creator="spring" javascript="makecommentaction">
			<param name="beanName" value="makeCommentAction" />
		</create>
		<create creator="spring" javascript="deletetopicaction">
			<param name="beanName" value="deleteTopicAction" />
		</create>
		<create creator="spring" javascript="deletecourseaction">
			<param name="beanName" value="deleteCourseAction" />
		</create>
		<create creator="spring" javascript="changeinfoaction">
			<param name="beanName" value="changeInfoAction" />
		</create>
		<create creator="spring" javascript="applycourseaction">
			<param name="beanName" value="applyCourseAction" />
		</create>
		<create creator="spring" javascript="searchAction">
			<param name="beanName" value="searchAction" />
		</create>
		<create creator="spring" javascript="handleapplicationaction">
			<param name="beanName" value="handleApplicationAction" />
		</create>
		<create creator="spring" javascript="handlefriendaction">
			<param name="beanName" value="handleFriendAction" />
		</create>
		<create creator="spring" javascript="chattingaction">
			<param name="beanName" value="chattingAction" />
		</create>
		<create creator="spring" javascript="sendmessageaction">
			<param name="beanName" value="sendMessageAction" />
		</create>
		<create creator="spring" javascript="flashinfoaction">
			<param name="beanName" value="flashInfoAction" />
		</create>
		<convert converter="bean" match="com.tutoring.bean.SearchResult">
		</convert>
		<convert converter="bean" match="com.tutoring.bean.SearchUserResult">
		</convert>
		<create creator="spring" javascript="searchFactors">
			<param name="beanName" value="searchFactors" />
		</create>
		<convert match="com.tutoring.bean.SearchFactors" converter="bean"
			javascript="searchFactors"></convert>
	</allow>
</dwr>